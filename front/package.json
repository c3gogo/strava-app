{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.26.11",
    "react": "0.0.0-experimental-241c4467e",
    "react-dom": "0.0.0-experimental-241c4467e",
    "react-relay": "0.0.0-experimental-8cc94ddc",
    "react-scripts": "3.2.0",
    "relay-runtime": "^9.0.0"
  },
  "devDependencies": {
    "babel-plugin-relay": "^9.0.0",
    "graphql": "^14.6.0",
    "relay-compiler": "^9.0.0"
  },
  "scripts": {
    "start": "npm run relay && react-scripts start",
    "build": "npm run relay && react-scripts build",
    "relay": "relay-compiler --schema ./src/schema/schema.graphql --src ./src/ --watchman false $@",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "babel": {
    "plugins": [
      "relay"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
